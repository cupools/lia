{
  "name": "lia",
  "version": "2.0.0",
  "description": "Sprite tool, bases on sprcify pictures, builds both sprite images and stylesheet files",
  "main": "main.js",
  "bin": {
    "lia": "./bin/lia"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test node_modules/.bin/nyc --reporter lcov --reporter text --report-dir test/coverage --require babel-core/register node_modules/.bin/mocha test/lia.test.js && rm -rf .nyc_output",
    "coverage": "cat test/coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "dev": "rm -rf lib && node node_modules/babel-cli/bin/babel src -d lib -D -w"
  },
  "author": "liyh <cupools@gmail.com>",
  "license": "MIT",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "chokidar": "^1.6.0",
    "colors": "^1.1.2",
    "ejs": "^2.5.1",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.3",
    "images": "^3.0.0",
    "layout": "^2.2.0",
    "lodash.sortby": "^4.6.1",
    "minimatch": "^3.0.3",
    "pngjs": "^3.0.0",
    "psd": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.11.4",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "cross-env": "^2.0.0",
    "mocha": "^2.3.3",
    "nyc": "^8.1.0",
    "postcss": "^5.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cupools/lia"
  },
  "bugs": {
    "url": "https://github.com/cupools/lia/issues"
  },
  "homepage": "https://github.com/cupools/lia",
  "keywords": [
    "sprite",
    "image",
    "spritesheet",
    "css"
  ]
}