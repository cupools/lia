{
  "name": "lia",
  "version": "1.2.1",
  "description": "Sprite tool, bases on sprcify pictures, builds both sprite pictures and stylesheet files",
  "main": "main.js",
  "bin": {
    "lia": "./bin/lia"
  },
  "scripts": {
    "test": "babel-node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha --dir test/coverage --root src --include-all-sources true -- --compilers js:babel-core/register -i test/*.test.js",
    "coverage": "cat test/coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "dev": "babel src -d lib -D -w"
  },
  "author": "liyh <cupools@gmail.com>",
  "license": "MIT",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "chokidar": "^1.6.0",
    "colors": "^1.1.2",
    "es6-template-strings": "^2.0.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.3",
    "images": "^3.0.0",
    "layout": "^2.2.0",
    "lodash.sortby": "^4.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cupools/lia"
  },
  "bugs": {
    "url": "https://github.com/cupools/lia/issues"
  },
  "homepage": "https://github.com/cupools/lia",
  "keywords": [
    "sprite",
    "image",
    "spritesheet",
    "css"
  ]
}