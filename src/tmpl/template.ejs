<% items.map(item => { -%>
.<%= item.name %> {
    width: <%= item.size.width + unit %>;
    height: <%= item.size.height + unit %>;
    background: url(<%= path %>) no-repeat;
    background-size: <%= size.width + unit %> <%= size.height + unit %>;
    background-position: <%= -item.x + unit %> <%= -item.y + unit %>;
}
<% }) %>