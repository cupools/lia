#!/usr/bin/env node
'use strict';

let command = process.argv[2];

switch(command) {
    case '-h':
    case 'help':
        require('../lib/cmd/help').default();
        break;
    case 'init':
        require('../lib/cmd/init').default();
        break;
    case 'here':
        require('../lib/cmd/here').default();
        break;
    case '-w':
    case 'watch':
        require('../lib/cmd/watch').default();
        break;
    default:
        require('../lib/cmd/main').default();
}
