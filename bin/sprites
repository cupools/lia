#!/usr/bin/env node

'use strict';

var fs = require('fs');
var path = require('path');
var Sprites = require('../main');
var confPath = path.resolve(process.cwd(), 'sprites_conf.js')
var config = [];

try {
    config = require(confPath);
} catch(e) {
    console.log('[warn]: sprites_conf.js not Found.');
}

config.map(conf => {
    var sp = new Sprites(conf);
    sp.run();
});

